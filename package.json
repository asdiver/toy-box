{
  "name": "toy-box",
  "version": "0.0.0",
  "description": "toy-box",
  "author": "asdiver",
  "main": "./dist/main/index.js",
  "scripts": {
    "dev": "chcp 65001 && vite",
    "dev:rebuild": "npm run build:electron && npm run dev",
    "dev:electron": "electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:electron": "tsc --project ./src/tsconfig.electron.json",
